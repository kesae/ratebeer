<h1>New membership</h1>
<%= form_with model: @membership do |form| %>
  <%= form.label :beer_club_id, "club:" %>
  <%= form.select :beer_club_id, options_from_collection_for_select(@beer_clubs, :id, :name) %>
  <br>
  <%= form.submit %>
<% end %>
<div>
  <%= link_to "Back to memberships", memberships_path %>
</div>
